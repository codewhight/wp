function min(a,b)
{
    if(a < b)
        return a;
    else if(b < a)
        return b;
    else
        return a;
}
var num1 = 10, num2 = 30;
console.log("The smaller number of num1 and num2 is", min(num1,num2));

//hw2
function array_min(a)
{
    let min = a[0];
    for(let i = 1;i < a.length;i++)
    {
        if(min > a[i])
            min = a[i];
    }
    return min;
}

var a = [7, 3, 23, 5, 39, 1];
console.log("The smaller number of array a is", array_min(a));
